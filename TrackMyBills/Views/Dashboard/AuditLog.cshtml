@model IEnumerable<TrackMyBills.Models.AuditModel>
@{
    ViewBag.Title = "Audit Log";
}
<h2>
    Audit Log</h2>
<table>
    <thead>
        <tr>
            <th>
                Date
            </th>
            <th>
                Type
            </th>
            <th>
                Text
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.AuditDate.ToString("dd MMM yyyy")
                </td>
                <td>
                    @(((TrackMyBills.Models.AuditType)item.AuditType).ToString())
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AuditText)
                </td>
            </tr>
        }
    </tbody>
</table>
